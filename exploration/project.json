{
  "name": "exploration",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "exploration/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/exploration",
        "main": "exploration/src/index.ts",
        "tsConfig": "exploration/tsconfig.lib.json",
        "assets": [
          "exploration/*.md",
          {
            "input": "./exploration/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./exploration/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./exploration",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./exploration",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "exploration/jest.config.ts"
      }
    }
  },
  "tags": []
}
